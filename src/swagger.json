    "paths": {
        "/decompose": {
            "post": {
                "tags": [
                    "Decomposed"
                ],
                "summary": "Decompose Number",
                "description": "you can set a number to decompose and show primes too.",
                "requestBody": {
                    "content": {
                        "application/json": {
                            "schema": {
                                "type": "object",
                                "properties": {
                                    "number": "number"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "divisibleNumbers": "number",
                        "primeNumbers": "number"
                    },
                    "500": {
                        "description": "Zero or Null number"
                    }
                }
            },
            "get": {
                "tags": [
                    "Decomposed"
                ],
                "summary": "List all decomposed number",
                "description": "List all decomposed number",
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "divisibleNumbers": {
                                                "type": "array"
                                            },
                                            "primeNumbers": {
                                                "type": "array"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}